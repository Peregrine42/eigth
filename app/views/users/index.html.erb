<h1>Users</h1>

<hr>
<%= link_to "New", new_user_path %>
<hr>
<table style="text-align: left; table-layout: fixed; width: 250px">
  <tr><th>Name</th><th>Role</th></tr>
  <% @resources.each do |resource| %>
    <tr>
      <td><%= link_to resource.name, user_path(resource.id) %></td>
      <td><%= link_to resource.role, user_path(resource.id) %></td>
    </tr>
  <% end %>
</table>
